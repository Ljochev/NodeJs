<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student-display</title>
</head>
<body>
<%- include("partials/header");%>
    <table border="1px" width="100%">
    <tr>
        <th>Name</th>
        <th>Last Name</th>
        <th>Average</th>
        <th>Link to student</th>
    </tr>
    <% fileData.forEach((student, i) => { %>
        <tr>
            <th><%=student.name%></th>
            <th><%=student.lastName%></th>
            <th><%=student.averageGrade%></th>
            <th><a href="/brishi?i=<%=`${i}`%>">Brisi</a></th>
        </tr>
    <%}); %>
    </table>
    <button><a href="/form">Enter new Student</a></button>
</body>
</html>

